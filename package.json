{
  "name": "nano_front",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "sui-bundler dev",
    "co": "sui-mono commit",
    "lint": "sui-lint js && sui-lint sass",
    "precommit": "sui-precommit run",
    "ssr": "sui-bundler build -C && sui-ssr build -C && node --inspect ./server"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@s-ui/component-peer-dependencies": "1",
    "@s-ui/domain": "1",
    "@s-ui/hoc": "1",
    "@s-ui/i18n": "1",
    "@s-ui/react-initial-props": "1",
    "classnames": "2.2.5",
    "isomorphic-fetch": "2.2.1",
    "reset-css": "3.0.0"
  },
  "devDependencies": {
    "@s-ui/bundler": "2",
    "@s-ui/ssr": "1",
    "@s-ui/lint": "2",
    "@s-ui/mono": "1",
    "@s-ui/precommit": "2",
    "husky": "0.13.4"
  },
  "config": {
    "validate-commit-msg": {
      "types": "@s-ui/mono/src/types"
    }
  },
  "sui-bundler": {
    "vendor": [
      "react",
      "react-dom",
      "react-router",
      "isomorphic-fetch"
    ],
    "offline": {
      "whitelist": [
        "::all::"
      ],
      "runtime": [{
        "urlPattern": "svg-loaders\\.surge\\.sh",
        "handler" : "cacheFirst"
      }]
    }
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/@s-ui/lint/eslintrc.js"
    ]
  },
  "sasslintConfig": "./node_modules/@s-ui/lint/sass-lint.yml"
}
